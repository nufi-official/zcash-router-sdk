{
  "name": "@zcash-router-sdk/core",
  "version": "0.1.0",
  "description": "A TypeScript SDK for asset routing",
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run typecheck && pnpm run test && pnpm run build"
  },
  "keywords": [
    "asset",
    "route",
    "router",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkviet/zcash-router-sdk.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/tkviet/zcash-router-sdk/issues"
  },
  "homepage": "https://github.com/tkviet/zcash-router-sdk#readme",
  "dependencies": {
    "@defuse-protocol/one-click-sdk-typescript": "^0.1.14"
  }
}
