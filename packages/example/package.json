{
  "name": "@zcash-router-sdk/example-app",
  "version": "0.1.0",
  "description": "Example React app demonstrating zcash-router-sdk usage",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "vite --host 0.0.0.0 --port $PORT",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "engines": {
    "node": "18.x",
    "pnpm": "8.x"
  },
  "dependencies": {
    "@zcash-router-sdk/core": "workspace:*",
    "@zcash-router-sdk/solana-account-mnemonic": "workspace:*",
    "@zcash-router-sdk/zcash-account-mnemonic": "workspace:*",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@solana/web3.js": "^1.95.8",
    "bip39": "^3.1.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "process": "^0.11.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "stream-browserify": "^3.0.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@vitejs/plugin-react": "^4.3.4",
    "crypto-browserify": "^3.12.0",
    "typescript": "^5.7.2",
    "util": "^0.12.5",
    "vite": "^6.0.3",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1"
  }
}
