{
  "name": "@asset-route-sdk/zcash-core",
  "version": "0.1.0",
  "description": "Zcash core account management library",
  "author": "",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run typecheck && pnpm run test && pnpm run build"
  },
  "keywords": [
    "zcash",
    "transparent",
    "account",
    "blockchain",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tkviet/asset-route-sdk.git",
    "directory": "packages/zcash-core"
  },
  "bugs": {
    "url": "https://github.com/tkviet/asset-route-sdk/issues"
  },
  "homepage": "https://github.com/tkviet/asset-route-sdk#readme",
  "dependencies": {
    "@asset-route-sdk/core": "workspace:*",
    "@chainsafe/webzjs-keys": "https://github.com/tkviet/WebZjs/releases/download/0.3.0/chainsafe-webzjs-keys-0.3.0.tgz",
    "@chainsafe/webzjs-wallet": "https://github.com/tkviet/WebZjs/releases/download/0.3.0/chainsafe-webzjs-wallet-0.3.0.tgz",
    "bip39": "^3.1.0"
  }
}
